{
  "name": "Operations",
  "description": "What user can do, dashboard <br>activities, type of requests ",
  "plural": "Operations",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "CustomDescription": {
      "descriptions": {
        "find": "Find a operation by where clause",
        "findById": "Find a operation by id",
        "create": "Create a new operation in application "
      }
    }
  },
  "properties": {
    "id": {
      "type": "number",
      "id": true,
      "description": "Operation id"
    },
    "name": {
      "type": "string",
      "required": true,
      "description": "Operation name"
    },
    "description": {
      "type": "string",
      "description": "Operation operative description"
    },
    "slug": {
      "type": "string",
      "required": true,
      "description": "Operation navigation bar url"
    },
    "view": {
      "type": "string",
      "required": true,
      "description": "filename with html extension of the view in views folder on client"
    },
    "controllername": {
      "type": "string",
      "required": true,
      "description": "controller name in controllerjs file "
    },
    "controllerjs": {
      "type": "string",
      "required": true,
      "description": "filename with js extension in controller client folder"
    },
    "dependencies": {
      "type": "string",
      "description": "filename list of external controller dependencies",
      "oracle": {
        "columnName": "DEPENDENCIES",
        "dataType": "NCLOB"
      }
    },
    "causelist": {
      "type": "string",
      "description": "List of causes autogenerated by requeststypes",
      "oracle": {
        "columnName": "CAUSELIST",
        "dataType": "NCLOB"
      }
    },
    "requeststypesid": {
      "type": "string",
      "description": "Operation type"
    },
    "requeststatesid": {
      "type": "string",
      "description": "Operation state"
    },
    "operationsvalidationid": {
      "type": "number",
      "description": "If operation is a request this id is relative of operation associated to user that validate and it's use to activate supervision on user to activate request route tree"
    },
    "icon": {
      "type": "string",
      "description": "The operation icon"
    },
    "isfortimecards": {
      "type": "boolean",
      "description": "The operation icon"
    }
  },
  "validations": [],
  "relations": {
    "operationsactivations": {
      "type": "hasMany",
      "model": "Operationsactivation",
      "foreignKey": "operationsid",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    },
    "graphs": {
      "type": "hasMany",
      "model": "Graphs",
      "foreignKey": "operationsid",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    },
    "requests": {
      "type": "hasMany",
      "model": "Requests",
      "foreignKey": "operationsid",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    },
    "users": {
      "type": "hasMany",
      "model": "Users",
      "foreignKey": "operationsid",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      },
      "through": "UsersOperations",
      "keyThrough": "operationsid"
    },
    "messages": {
      "type": "hasMany",
      "model": "Messages",
      "foreignKey": "operationsid",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    },
    "messagesforusers": {
      "type": "hasMany",
      "model": "Messages",
      "foreignKey": "operationsid",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      },
      "through": "UsersOperationsMessages",
      "keyThrough": "operationsid"
    },
    "services": {
      "type": "hasMany",
      "model": "Services",
      "foreignKey": "operationsid",
      "options": {
        "disableInclude": false
      },
      "through": "OperationsServices",
      "keyThrough": "operationsid"
    }
  },
  "acls": [],
  "methods": {}
}
